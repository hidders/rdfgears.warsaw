<processor label="Twitter Language Detector" category="Custom Java">
	<description>
	This is a custom java function that detects twitter languages based on a Twitter username.
	</description>
	<inputs>
		<function type="custom-java"> 
			<param name="implementation" value="nl.tudelft.rdfgears.rgl.function.imreal.TwitterLanguageDetector"/>
		</function>
		<data iterate="false" name="username" label="Username">
			<type><var name="username"/></type>
		</data>
		<data iterate="false" name="uuid" label="UUID">
			<type><var name="uuid"/></type>
		</data>
	</inputs> 
	<output/>
</processor>

